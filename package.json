{
  "name": "discover-xyz",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "pkill -f 'next dev' || true && cross-env NODE_OPTIONS=--no-deprecation PORT=3026 next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "reset-db": "cross-env NODE_OPTIONS=--no-deprecation node scripts/reset-db.js",
    "start": "cross-env NODE_OPTIONS=--no-deprecation PORT=3026 next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "sync": "fedsync --all",
    "import": "pnpm run sync && payload run scripts/import-fedsync.mjs",
    "cron:fedsync": "tsx src/scripts/fedsync-cron.ts"
  },
  "dependencies": {
    "@better-auth/client": "0.0.2-alpha.3",
    "@daveyplate/better-auth-ui": "^1.4.7",
    "@payloadcms/admin-bar": "3.45.0",
    "@payloadcms/db-mongodb": "3.45.0",
    "@payloadcms/live-preview-react": "3.45.0",
    "@payloadcms/next": "3.45.0",
    "@payloadcms/payload-cloud": "3.45.0",
    "@payloadcms/plugin-form-builder": "3.45.0",
    "@payloadcms/plugin-nested-docs": "3.45.0",
    "@payloadcms/plugin-redirects": "3.45.0",
    "@payloadcms/plugin-search": "3.45.0",
    "@payloadcms/plugin-seo": "3.45.0",
    "@payloadcms/richtext-lexical": "3.45.0",
    "@payloadcms/ui": "3.45.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^5.83.0",
    "better-auth": "^1.3.4",
    "better-auth-harmony": "^1.2.5",
    "chalk": "^5.4.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "commander": "^14.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "fedsync-standalone": "github:erawk26/fedsync",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.378.0",
    "next": "15.3.3",
    "next-sitemap": "^4.2.3",
    "ora": "^8.2.0",
    "p-limit": "^6.2.0",
    "payload": "3.45.0",
    "payload-auth": "^1.6.4",
    "prism-react-renderer": "^2.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.45.4",
    "sharp": "0.32.6",
    "sonner": "^2.0.6",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@playwright/test": "1.50.0",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "4.5.2",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.3.0",
    "jsdom": "26.1.0",
    "mongodb": "^6.17.0",
    "playwright": "1.50.0",
    "playwright-core": "1.50.0",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "overrides": {
      "esbuild": ">=0.25.0",
      "@eslint/plugin-kit": ">=0.3.4"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "fedsync-standalone",
      "sharp",
      "unrs-resolver"
    ],
    "ignoredBuiltDependencies": [
      "fedsync-standalone"
    ]
  }
}
